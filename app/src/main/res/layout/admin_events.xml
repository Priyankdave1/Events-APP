<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="100dp"

    android:layout_margin="10dp"
    android:clickable="true"

    android:orientation="vertical"
    app:cardCornerRadius="45dp"
    app:cardElevation="20dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/pastelgreen"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/sport_up"
                android:layout_width="230dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:fontFamily="sans-serif-black"
                android:singleLine="true"
                android:text="sdfsdfsdfdssssssds"
                android:textColor="@color/nephritis"

                android:textSize="22dp"
                app:autoSizeMaxTextSize="26dp"
                app:autoSizeMinTextSize="24dp" />

            <TextView
                android:id="@+id/capacity_up"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/sport_up"
                android:autoSizeMinTextSize="10dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:singleLine="true"
                android:text="Capacity"

                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_marginRight="10dp">


            <TextView
                android:id="@+id/venue_up"

                android:layout_width="230dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="16dp"
                android:layout_marginTop="40dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:singleLine="true"
                android:text="venukljdafklajfkljadslkfjaklfjakl;jfklajdlklafjadklklsfje"

                android:textColor="@color/black"
                app:autoSizeMaxTextSize="26dp"
                app:autoSizeMinTextSize="24dp" />

            <TextView
                android:id="@+id/spots_up"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="38dp"
                android:layout_marginEnd="9dp"
                android:layout_toEndOf="@+id/venue_up"
                android:autoSizeMinTextSize="10dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:singleLine="true"
                android:text="spodsdsdsts"

                android:textSize="15dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:orientation="horizontal"

            />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/timee_up"
                android:layout_width="175dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"

                android:layout_marginTop="68dp"
                android:layout_marginEnd="172dp"

                android:autoSizeMinTextSize="10dp"
                android:fontFamily="sans-serif-medium"
                android:singleLine="true"
                android:text="time"

                android:textSize="15dp" />

        </RelativeLayout>



    </RelativeLayout>




</androidx.cardview.widget.CardView>
